                                            <label for="input-diesel-fuel-unit" class="visible-xs col-xs-3 control-label">Measurement</label>
                                            <div class="col-sm-12 col-xs-9">
                                                <select class="bs-select" data-width="100px" id="input-diesel-fuel-unit">
                                                    <option value="gallons">Gallons</option>
                                                    <option value="pounds" selected>Pounds</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                        <textarea class="form-control grow" rows="1" id="input-diesel-fuel-notes" placeholder="Notes"></textarea>
                                    </div>
                                </div>

                                <!-- Gasoline -->
                                <div id="input-gasoline" class="form-group border-left border-green calculate-fuel-row" data-fuel-capacity="3700">
                                    <div class="col-sm-2 control-label">
                                        <label>Gasoline</label>
                                        <p><small>Capacity <strong>3,700</strong> gallons</small></p>
                                    </div>
                                    <div class="col-sm-3">
                                        <div class="btn-group btn-group-sm status-btns two-btns">
                                            <button type="button" class="btn btn-success selected">Adequate</button>
                                            <button type="button" class="btn btn-danger">Short</button>
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label for="input-gasoline-amount-short" class="visible-xs col-xs-3 control-label">Amount Short</label>
                                            <div class="col-sm-12 col-xs-9">
                                                <input type="number" class="form-control amount-short" id="input-gasoline-amount-short" value="0">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <label for="input-gasoline-unit" class="visible-xs col-xs-3 control-label">Measurement</label>
                                            <div class="col-sm-12 col-xs-9">
                                                <select class="bs-select" data-width="100px" id="input-gasoline-unit">
                                                    <option value="gallons">Gallons</option>
                                                    <option value="pounds" selected>Pounds</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
